<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889  -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="ApplicationName" value="USAID.MapCODLocationsPreviewer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="ApplicationDomain" value="alpha.itos.uga.edu"  xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EnableDocumentGeneration" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="DocumentStorageLocation" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	</appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <compilation debug="false" xdt:Transform="SetAttributes(debug)" />
    <customErrors mode="Off" defaultRedirect="~/Shared/Error.cshtml" xdt:Transform="SetAttributes(mode,defaultRedirect)" />
  </system.web>
</configuration>